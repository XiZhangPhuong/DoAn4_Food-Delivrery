<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fastfooddelivery" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Dialog.showNotification"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FastFoodDelivery" >
        <activity
            android:name=".GioHangActivity"
            android:exported="false" />

        <activity
            android:name=".Activity.NotificationActivity"
            android:exported="false" />
        <activity
            android:name=".UpdatePasswordActivity"
            android:exported="false" />
        <activity
            android:name=".VerifyActivity"
            android:exported="true" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Activity.CartActivity" />
        <activity android:name=".LoginAndSingupActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>